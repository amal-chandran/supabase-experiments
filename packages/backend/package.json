{
  "name": "@supabase-start/db",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./src/*.ts"
    }
  },
  "scripts": {
    "build": "tsdown",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "npx supabase migrations up",
    "db:reset": "npx supabase db reset",
    "generate:signing-key": "tsx scripts/generate-signing-key.ts",
    "status": "npx supabase status",
    "dev": "npx supabase start",
    "reboot": "npx supabase stop && npx supabase start"
  },
  "devDependencies": {
    "drizzle-kit": "^0.31.2",
    "@types/pg": "^8.11.11",
    "tsdown": "^0.15.5",
    "tsx": "^4.19.2"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "drizzle-orm": "^0.44.2",
    "pg": "^8.14.1",
    "dotenv": "^17.2.2",
    "zod": "^4.1.11",
    "jose": "^5.9.6",
    "@supabase-start/auth": "*"
  }
}