{
  "name": "@supabase-start/auth",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./auth": {
      "types": "./dist/auth.d.ts",
      "default": "./src/auth.ts"
    },
    "./app": {
      "types": "./dist/app.d.ts",
      "default": "./src/app.ts"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./src/*.ts"
    }
  },
  "type": "module",
  "scripts": {
    "build": "tsdown",
    "check-types": "tsc -b",
    "compile": "bun build --compile --minify --sourcemap --bytecode ./src/app.ts --outfile server",
    "dev": "tsx watch src/app.ts",
    "start": "node dist/app.js",
    "auth:generate": "npx @better-auth/cli generate"
  },
  "devDependencies": {
    "@types/node": "^22.13.11",
    "@types/pg": "^8.11.11",
    "drizzle-kit": "^0.31.2",
    "tsdown": "^0.15.5",
    "tsx": "^4.19.2",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@better-auth/cli": "^1.3.34",
    "@hono/node-server": "^1.14.4",
    "better-auth": "^1.3.34",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.44.2",
    "hono": "^4.8.2",
    "jose": "^5.9.6",
    "pg": "^8.14.1",
    "zod": "^4.1.11"
  }
}